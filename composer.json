{
    "name": "agentsib/jsonrpc-bundle",
    "description": "Symfony2 JsonRpcBundle",
    "type": "symfony-bundle",
    "keywords": ["json", "rpc", "json-rpc", "api"],
    "homepage": "http://github.com/agentsib/jsonrpc-bundle",
    "authors": [
        {
            "name": "AgentSIB",
            "email": "agentsib@gmail.com",
            "homepage": "http://agentsib.ru"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/agentsib/jsonrpc"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "agentsib/jsonrpc": "~1.0"
    },
    "autoload": {
        "psr-0": { "AgentSIB\\JsonRpcBundle\\": "" }
    },
    "target-dir": "AgentSIB/JsonRpcBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }

}
